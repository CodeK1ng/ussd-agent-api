{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"resolveJsonModule": true,
		"outDir": "./build/",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	}
}
